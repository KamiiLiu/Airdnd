{
  "version": "1.0",
  "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "leaflet@1.8.0",
            "destination": "wwwroot/lib/leaflet/",
            "files": [
                "leaflet.js",
                "leaflet.css"
            ]
        },
        {
            "library": "leaflet.markercluster@1.5.3",
            "destination": "wwwroot/lib/leaflet/",
            "files": [
                "leaflet.markercluster.js",
                "MarkerCluster.Default.css"
            ]
        },
        {
            "library": "bootstrap@5.2.0",
            "destination": "wwwroot/lib/bootstrap/",
            "files": [
                "js/bootstrap.min.js",
                "css/bootstrap.min.css.map",
                "css/bootstrap.min.css"
            ]
        },
        {
            "library": "Swiper@8.3.2",
            "destination": "wwwroot/lib/Swiper/",
            "files": [
                "swiper-bundle.min.js"
            ]
        },
        {
            "library": "font-awesome@6.2.0",
            "destination": "wwwroot/lib/font-awesome/"
        },
        {
            "library": "meyer-reset@2.0",
            "destination": "wwwroot/lib/meyer-reset/",
            "files": [
                "reset.min.css"
            ]
        },
        {
            "library": "axios@0.27.2",
            "destination": "wwwroot/lib/axios/"
        },
        {
            "library": "vue@2.6.14",
            "destination": "wwwroot/lib/vue/"
        },
        {
            "library": "popper.js@1.14.7",
            "destination": "wwwroot/lib/popper.js/",
            "files": [
                "popper.min.js"
            ]
        },
        {
            "provider": "cdnjs",
            "library": "jquery-cookie@1.4.1",
            "destination": "wwwroot/lib/jquery-cookie/",
            "files": [
                "jquery.cookie.min.js"
            ]
        }
    ]
}